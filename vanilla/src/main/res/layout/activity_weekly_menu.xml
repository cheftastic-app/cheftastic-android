<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_weekly_menu_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.cheftastic.vanilla.WeeklyMenuActivity">

    <FrameLayout
        android:id="@+id/activity_weekly_menu_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <fragment
        android:id="@+id/activity_weekly_menu_fragment_nav_bar"
        android:name="app.cheftastic.vanilla.fragments.NavBarFragment"
        android:layout_width="@dimen/nav_bar_width"
        android:layout_height="match_parent"
        android:layout_gravity="start" />
</androidx.drawerlayout.widget.DrawerLayout>